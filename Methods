   private String getElementName(WebElement element) {
        StackTraceElement[] stackTrace = Thread.currentThread().getStackTrace();
        String callerMethod = Arrays.stream(stackTrace)
                .filter(ste -> !ste.getClassName().equals(Webkeywords.class.getName()))
                .findFirst()
                .map(StackTraceElement::getMethodName)
                .orElse("unknown");

        String elementString = element.toString();
        Pattern pattern = Pattern.compile("->\\s+(\\w+):");
        Matcher matcher = pattern.matcher(elementString);
        String elementType = matcher.find() ? matcher.group(1) : "element";

        return callerMethod + "_" + elementType;
    }
